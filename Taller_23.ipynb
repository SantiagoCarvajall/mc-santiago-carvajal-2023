      "source": [
        "import numpy as np\n",
        "from scipy.interpolate import lagrange\n",
        "\n",
        "\n",
        "x = np.array([0, 1, 2, 3, 4])\n",
        "y = np.array([1, 0.9, -1, -2.3, 1.8])\n",
        "\n",
        "\n",
        "polinomio = lagrange(x, y)\n",
        "\n",
        "\n",
        "print(f\"El polinomio de interpolación de Lagrange es: {polinomio}\")\n"
      ]
    }
  ]
